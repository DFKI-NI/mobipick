<?xml version="1.0" ?>

<launch>
  <arg name="namespace" default="" />
  <arg name="paused" default="false" />

  <include file="$(find gazebo_ros)/launch/empty_world.launch" />

  <!-- Convert an xacro and put on parameter server -->
  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find mobipick_description)/urdf/ur5/ur5_joint_limited_robot_sim.urdf.xacro" />

  <!-- Spawn a robot into Gazebo  -->
  <node if="$(arg paused)" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model ur5 -gazebo_namespace /$(arg namespace)/gazebo -J shoulder_pan_joint 0.0 -J shoulder_lift_joint -1.570796327 -J elbow_joint 0.0 -J wrist_1_joint -1.570796327 -J wrist_2_joint 0.0 -J wrist_3_joint 0.0" /> 
  <node unless="$(arg paused)" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model ur5 -gazebo_namespace /$(arg namespace)/gazebo" />

</launch>
